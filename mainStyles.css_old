    margin: 0px 0px 15px 48px;
}
.email-form {
    margin-top: 0px;
}
.email-form textarea {
    border-radius: 5px;
    margin-bottom: 10px;
    width: 55% !important;
    height: 120px !important;
}
input[type="submit"],
input[type="reset"] {
    border: 1px solid #FFFFFF;
    padding: 15px;
    border-radius: 10px;
    margin-left: 115px;
    margin-top: 20px;
    background-color: #313a58;
    color: #FFFFFF;
    box-shadow: inset 0px 0px 10px rgba(255, 255, 255, 0.8), 0px 3px 7px darkgrey;
    transition: background-color .5s ease-in-out;
}
input[type="submit"]:hover,
input[type="reset"]:hover {
    border: 1px solid grey;
    background-color: #5d7cc0;
    cursor: pointer;
    box-shadow: inset 0px 0px 20px rgba(255, 255, 255, 0.8), 0px 6px 2px darkgrey;
}
input[type="submit"]:focus,
input[type="reset"]:focus {
    border: 1px solid orange;
    background-color: #5d7cc0;
    cursor: pointer;
    box-shadow: inset 0px 0px 20px rgba(255, 255, 255, 0.8), 0px 6px 2px orange;
}
#searchFAQs,
.email-form input[type="text"],
.email-form input[type="email"],
.email-form input[type="tel"],
.email-form textarea {
    padding: 5px;
    border-radius: 5px;
    /*used -ms-linear-gradient from  page 575 */
    
    background: -ms-linear-gradient(rgb(232, 232, 232) 0%, rgb(255, 255, 255) 85%);
    /* added mozilla linear gradient for progessive enhancement*/
    
    background: -moz-linear-gradient(rgb(232, 232, 232) 0%, rgb(255, 255, 255) 85%);
    /*used webkit linear gradient version from page 572*/
    
    background: -webkit-gradient(linear, center, 0% center, from(rgb(232, 232, 232)), to(rgb(255, 255, 255)));
    background: linear-gradient(rgb(232, 232, 232) 0%, rgb(255, 255, 255) 85%);
    /* background: progid:DXImageTransform.Microsoft.gradient( gradientType=0, startColorStr=#FFFFFF, endColorStr=#E9E9E9 ) */
    
    ;
    border-style: dashed;
    -moz-transition: box-shadow 0.5s ease-in-out;
    -webkit-transition: box-shadow 0.5s ease-in-out;
    -ms-transition: box-shadow 0.5s ease-in-out;
    transition: box-shadow 0.5s ease-in-out;
}
#searchFAQs:focus,
.email-form input:focus,
.email-form textarea:focus {
    outline: none;
    box-shadow: inset 0px 0px 20px rgba(235, 169, 55, 0.9);
    border-style: solid;
}
div .serviceCatgs2 ul {
    list-style-type: none;
    position: relative;
    left: -40px;
}
div .serviceCatgs2 ul li {
    display: inline;
    padding-left: 11px;
    padding-right: 11px;
}
div .serviceCatgs2 ul li a {
    font-size: 16px;
    color: #475379;
    -moz-transition: color .33s ease-in-out;
    -webkit-transition: color .33s ease-in-out;
    transition: color .33s ease-in-out;
}
div .serviceCatgs2 ul li a:hover {
    color: #FFCE00;
}
.serviceCatgs2 h3 {
    text-align: center;
    padding-bottom: 5px;
    margin-left: -45px;
}
/* animated header*/

header {
    background-image: url('images/compu-work.jpg');
    height: 145px;
    background-position: 0px 0px;
    background-repeat: no-repeat;
    overflow: hidden;
	animation: animatedBackground linear infinite 10s;
}
@-moz-keyframes animatedBackground {
    0% {
        background-position: 0px -60px;
    }
    100% {
        background-position: 0px -150px;
    }
}
@-webkit-keyframes animatedBackground {
    0% {
        background-position: 0px -60px;
    }
    100% {
        background-position: 0px -150px;
    }
}
@-o-keyframes animatedBackground {
    0% {
        background-position: 0px -60px;
    }
    100% {
        background-position: 0px -150px;
    }
}
