.hide-modal {
    display: none;
}
.show-modal {
    display: block;
    text-align: left;
    font-size: 14px;
    padding: 30px;
    box-sizing: border-box;
    position: absolute;
    left: 12.5%;
    top: 15%;
    width: 75%;
    border: 1px solid grey;
    background-color: rgba(255, 255, 255, 0.95);
    /*background-color: rgba(92,113,161,0.95);*/
    
    border-radius: 20px;
    box-shadow: 2px 2px 20px rgba(0, 0, 0, 0.75);
    color: #000000;
    letter-spacing: 1px;
    word-wrap: break-word;
    z-index: 1000000;
}
.show-modal p,
.show-modal h1 {
    font-size: 14px;
    font-family: 'Arial', sans-serif !important;
}
#dismiss {
    color: #000000;
    z-index: 1000000;
}
#dismiss:hover {
    cursor: pointer;
    color: red;
}