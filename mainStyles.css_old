}*/
.breaker {
    display: none;
}
.serviceCatgs-switch,
.serviceCatgs-switch3 {
    display: none;
}
.faqs-serviceCatgs-switch {
    display: none;
}
div.funny-talk p {
    width: 125%;
}
@media only screen and (max-width: 1024px) {
    .hRule:nth-of-type(1),
    .hRule:nth-of-type(2) {
        margin-top: 10px !important;
        width: 100% !important;
    }
    #rule-last {
        display: none;
    }
    div.serviceCatgs {
        display: none;
    }
    .serviceCatgs3 .image-one-2,
    .serviceCatgs3 .image-two-2,
    .serviceCatgs3 .image-three-2 {
        display: none;
    }
    .serviceCatgs-switch,
    .serviceCatgs-switch3 {
        display: block;
        width: 114.10%;
        height: auto;
        max-width: 114.10%;
    }
    .serviceCatgs-switch {
        margin-top: 4%;
    }
    .serviceCatgs-switch3 {
        margin-top: 0%;
    }
    .articleDiv1,
    .h2lfcTechIntro {
        width: 90%;
    }
    .lfcTech {
        left: -12%;
        width: 100%;
        top: 25%;
        color: white;
        letter-spacing: 1%;
        font-size: 250%;
        padding: 0% 0% 0% 17%;
        box-sizing: border-box;
    }
    .mlfcLogo {
        display: none;
    }
    div.hImage {
        position: relative;
        top: 80%;
        width: 100%;
    }
    .navList {
        padding: 0px 0px 0px 13%;
    }
    .container {
        width: 95%;
        z-index: 0;
    }
    header,
    nav,
    footer {
        width: 100%;
    }
    p {
        width: 98%;
        box-sizing: border-box;
    }
    main {
        width: 78%;
        box-sizing: border-box;
        padding: 1.25em 0px 0px 1.25em;
    }
    .h2lfcTechIntro,
    article.introToLFC,
    article.pcTips,
    div.pcTipsDiv {
        width: 100%;
        box-sizing: border-box;
    }
    aside {
        width: 20%;
    }
    div.locate {
        width: 100%;
    }
    div.locate div {
        width: 20%;
    }
    #aside-image {
        display: none;
        height: 0px;
    }
    .pcTipsDiv table tbody tr td img {
        display: none;
    }
    .nav-side li,
    .navFAQs li {
        padding: 14px 0px 14px 0%;
    }
    .nav-side li a,
    .navFAQs li a {
        font-size: 85%;
        padding: 7px 0% 7px 10%;
    }
    .nav-side li,
    .navFAQs li {
        transition: none;
    }
    .nav-side li:nth-of-type(6):hover {
        background-position: -20px;
        transition: background none;
    }
    .nav-side li:hover,
